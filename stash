proxy-groups:
  - name: "AI1"
    type: select
    proxies:
      - DIRECT
      - "Auto"
      - "HK-Auto"
      - "TW-Auto"
      - "JP-Auto"
      - "SG-Auto"
      - "KR-Auto"
      - "US-Auto"
      - "Others-Auto"
      - REJECT
  - name: "Auto"
    type: url-test
    use:
      - provider1
      - provider2
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "HK-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)HK|Hong Kong|香港"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "TW-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)TW|Taiwan|台灣|台湾"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "JP-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)JP|Japan|日本"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "SG-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)SG|Singapore|新加坡"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "KR-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)KR|Korea|韩国|韓國"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "US-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)US|United States|美国"
    url: http://www.gstatic.com/generate_204
    interval: 300
  - name: "Others-Auto"
    type: url-test
    use:
      - provider1
      - provider2
    filter: "(?i)Australia|UK|Germany|France|Canada|其他"
    url: http://www.gstatic.com/generate_204
    interval: 300

